<tr 
  class="category-row"
  data-categoryid="{{id}}">

  <th scope="row">{{id}}</th>
  <td>{{name}}</td>
  <td>{{format_date start}}</td>
  <td>{{format_time start}}</td>
  <td>{{format_date end}}</td>
  <td>{{format_time end}}</td>
  <td class="text-end">
    <button 
      class=" btn btn-sm btn-light" 
      type="button">
      <i class="far fa-edit"></i>
    </button>
  </td>
  <td class="text-end">
    <button 
      class=" btn btn-sm btn-light" 
      type="button">
      <i class="far fa-trash-alt"></i>
    </button>
  </td>
</tr>